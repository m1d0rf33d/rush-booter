<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<VBox fx:id="rootVBox" maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yondu.controller.ActivationController">
   <children>
      <HBox prefHeight="50.0" prefWidth="600.0">
         <children>
            <ImageView fx:id="logoImageView" fitHeight="50.0" fitWidth="75.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets />
               </HBox.margin></ImageView>
            <Label text="Rush POS Sync" textFill="#140101">
               <font>
                  <Font size="20.0" />
               </font>
               <HBox.margin>
                  <Insets left="30.0" top="20.0" />
               </HBox.margin>
            </Label>
         </children>
         <VBox.margin>
            <Insets left="60.0" top="20.0" />
         </VBox.margin>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #eeeeee;">
         <VBox.margin>
            <Insets left="120.0" right="30.0" />
         </VBox.margin>
         <children>
            <Label text="To activate this product please enter your merchant key." textOverrun="CLIP">
               <font>
                  <Font size="15.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="40.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="30.0" style="-fx-background-color: #CDD2D6;" text="Merchant key:">
                     <HBox.margin>
                        <Insets left="10.0" top="30.0" />
                     </HBox.margin>
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <TextField fx:id="merchantKeyTextField" prefHeight="30.0" prefWidth="300.0">
                     <HBox.margin>
                        <Insets top="30.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <VBox.margin>
            <Insets left="120.0" right="50.0" />
         </VBox.margin>
         <children>
            <Button fx:id="activateButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" style="-fx-background-radius: 30; -fx-background-color: #ffb600;" text="ACTIVATE" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="OPEN_HAND" />
               </cursor>
            </Button>
            <Button fx:id="cancelButton" mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" style="-fx-background-radius: 30; -fx-background-color: #CDD2D6;" text="CANCEL" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <cursor>
                  <Cursor fx:constant="OPEN_HAND" />
               </cursor>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
